{
  "name": "armino/binance-connector",
  "description": "Binance API connector for PHP",
  "type": "library",
  "keywords": [
    "binance",
    "binance api",
    "binance connector php"
  ],
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Armino\\BinanceConnector\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Armino\\BinanceConnector\\Tests\\": "tests"
    }
  },
  "authors": [
    {
      "name": "Armino Popp",
      "email": "devel@boioiong.com"
    }
  ],
  "require": {
    "php": ">=7.4|>=8.0",
    "ext-curl": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  }
}
